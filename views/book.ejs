<%-include partials/header.ejs%>
<%-include partials/main_nav.ejs%>
  <!--Main Nav Navigation-->
  
<main class="container">
  <!-- <div class="jumbotron">
    <h1 class="display-4">Welcome to the <%= title %> Page</h1>
    </div> -->
    <div class="row">
        <div class = "col-md-offset-3 col md-8">
            <h1> <%= title %></h1>
            <br/>
            <table class="table.table-bordered table-striped table-hover">
                <tr>
                    <th class="text-content">Name</th>
                    <th class="text-content">Author</th>
                    <th class="text-content">Published</th>
                    <th class="text-content">Description</th>
                    <th class="text-content">Price</th>
                </tr>
                <% for(let count=0; count <BooksList.length; count++) { %>
                    <tr>
                        <td class="text-center"><%= BooksList[count].name %></td>
                        <td class="text-center"><%= BooksList[count].author %></td>
                        <td class="text-center"><%= BooksList[count].published %></td>
                        <td class="text-center"><%= BooksList[count].description %></td>
                        <td class="text-center"><%= BooksList[count].price %></td>
                    </tr>
                <%}%>
            </table>

        </div>
    </div>
  </main>
  
